<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/green_light"
    tools:context=".Actitvity.Parcel_Tracking">

    <TextView
        android:id="@+id/checkInDate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="10dp"
        android:layout_alignParentTop="true"
        android:text="@string/checkin_date"
        android:textColor="@android:color/black"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentTop="true"
        android:layout_marginEnd="39dp"
        android:layout_marginTop="19dp"
        android:text="12/09/2018"
        android:textColor="@android:color/black"
        android:textSize="20sp"
        android:layout_alignParentRight="true"
        android:layout_marginRight="39dp" />

    <TextView
        android:id="@+id/batchNo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginTop="59dp"
        android:layout_marginLeft="10dp"
        android:text="@string/batch_no"
        android:textColor="@android:color/black"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/text1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/text"
        android:layout_alignTop="@+id/batchNo"
        android:text="233424343"
        android:textColor="@android:color/black"
        android:textSize="20sp"
        android:layout_alignRight="@+id/text" />
    <TextView
        android:id="@+id/totalShipment"
        android:layout_margin="12dp"
        android:layout_marginTop="10dp"
        android:layout_below="@id/batchNo"
        android:layout_width="wrap_content"
        android:textSize="20sp"
        android:textColor="@android:color/black"
        android:layout_height="wrap_content"
        android:text="@string/totalshipment"/>
    <TextView
        android:id="@+id/text2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="10dp"
        android:layout_below="@id/batchNo"
        android:layout_toRightOf="@id/totalShipment"
        android:text="12/09/2018"
        android:textColor="@android:color/black"
        android:textSize="20sp" />
    <TextView
        android:id="@+id/shipperInfo"
        android:layout_margin="12dp"
        android:layout_marginTop="10dp"
        android:layout_below="@id/totalShipment"
        android:layout_width="wrap_content"
        android:textSize="20sp"
        android:textColor="@android:color/black"
        android:layout_height="wrap_content"
        android:text="@string/shipper_info"/>

    <TextView
        android:id="@+id/text3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/text1"
        android:layout_marginTop="140dp"
        android:text="12/09/2018"
        android:textColor="@android:color/black"
        android:textSize="20sp"
        android:layout_alignLeft="@+id/text1" />

</RelativeLayout>